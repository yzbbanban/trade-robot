                <TextField fx:id="peoCount" disable="true" layoutX="380.0" layoutY="60.0" />

                <Label layoutX="14.0" layoutY="95.0" text="工站结果" />
                <TableView layoutX="14.0" layoutY="115.0" prefHeight="200.0" prefWidth="400.0">
